-- KRIPKE2

constructs: A B
defuncts: states atoms

axioms:

states == [0] &
atoms == [A,B] &

0 -> 1<+>4 &
1 -> 2 &
2 -> 1<+>2<+>3 &
3 -> 3 &
4 -> 5<+>6 &
5 -> 6<+>7<+>8 &
6 -> 6<+>8<+>9 &
7 -> 10 &
8 -> 8 &
9 -> 11 &
10 -> 7<+>8<+>10 &
11 -> 8<+>9<+>10 &

A -> branch[0,1,3,4,7,8,9] &
B -> branch[0,2,5,6,10,11] 
	    
terms: bisim --> [(1,4),(1,7),(1,9),(4,7),(4,9),(2,5),(2,6),(2,10),(2,11),
             --   (5,6),(5,10),(5,11),(6,10),(6,11),(3,8),(7,9),(10,11)]

